    "def sum_even_fibo(limit):\n",
    "    a,b = 0,1\n",
    "    even = []\n",
    "    while b <= limit:\n",
    "        if b % 2 == 0:\n",
    "            even.append(b)\n",
    "        c = b\n",
    "        b = a + b\n",
    "        a = c\n",
    "    return sum(even)"
   ]
